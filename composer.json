{
    "name": "topwisesoftware/artefato",
    "version": "22.19",
    "type": "project",
    "description": "TopWise Software CI4 Starter APP",
    "homepage": "https://topwise.com.br",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Joe Edman",
            "email": "joe.edman@gmail.com",
            "homepage": "https://topwise.com.br/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "codeigniter4/framework": "^4.0",
        "almasaeed2010/adminlte": "~3.1",
        "dompdf/dompdf": "^1.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "suggest": {
        "ext-fileinfo": "Improves mime type detection for files"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "support": {
        "forum": "https://topwise.com.br/produtos/artefato",
        "source": "https://github.com/topwisesoftware/artefato"
    }
}
